// this exists:
// - to help tooling find files, for development
// - For test type checking
{
  "compilerOptions": {
    "target": "esnext",
    "module": "none",
    "moduleResolution": "node",
    "strict": true,
    "plugins": [
      {
        "name": "typescript-tstl-plugin"
      }
    ],
    "types": [],
    "noEmit": true
  },
  "include": ["**/index.d.ts", "test"]
}
